# SPARC CLI Environment Configuration
# Copy this file to .env and fill in your values

# ===== CORE SERVICES =====

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Qdrant Configuration  
QDRANT_URL=http://localhost:6336
QDRANT_API_KEY=your_qdrant_api_key_if_needed

# ===== AI SERVICES =====

# Perplexity AI (for research agents)
PERPLEXITY_API_KEY=your_perplexity_api_key

# Mistral AI (for agents requiring Mistral models)
MISTRAL_API_KEY=your_mistral_api_key

# OpenAI (for embeddings and additional AI capabilities)
OPENAI_API_KEY=your_openai_api_key

# ===== DEVELOPMENT SETTINGS =====

# Default project namespace (for testing)
DEFAULT_PROJECT_ID=default_test_project

# Agent system settings
MAX_CONCURRENT_AGENTS=5
AGENT_TIMEOUT_MINUTES=10
AUTO_PHASE_PROGRESSION=true

# Logging level
LOG_LEVEL=INFO

# ===== OPTIONAL INTEGRATIONS =====

# GitHub (for repository integration)
GITHUB_TOKEN=your_github_token

# Docker settings
DOCKER_HOST=unix:///var/run/docker.sock
SPARC_NETWORK=sparc_network

# ===== SECURITY =====

# JWT secret for session management
JWT_SECRET=your_jwt_secret_key

# Encryption key for sensitive data
ENCRYPTION_KEY=your_encryption_key

# ===== RESEARCH CONFIGURATION =====

# Research settings for Perplexity integration
RESEARCH_MAX_TOKENS=4000
RESEARCH_TEMPERATURE=0.1
RESEARCH_RECENCY_FILTER=month

# ===== MEMORY SETTINGS =====

# Vector database settings
VECTOR_DIMENSION=1536
SIMILARITY_THRESHOLD=0.8
MAX_SEARCH_RESULTS=10

# Memory retention settings
MEMORY_RETENTION_DAYS=90
AUTO_CLEANUP_ENABLED=true